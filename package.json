    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.1",
    "liquid-node": "bleathem/liquid-node#v2.6.1-pf.1",
    "lodash": "^4.17.4",
    "matchdep": "~1.0.1",
    "mz": "^2.6.0",
    "npm-check-updates": "^2.12.1",
